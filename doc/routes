                   Prefix Verb   URI Pattern                                                                              Controller#Action
         v1_content_index GET    /v1/content(.:format)                                                                    v1/content#index {:format=>/json/}
                          POST   /v1/content(.:format)                                                                    v1/content#create {:format=>/json/}
               v1_content PATCH  /v1/content/:id(.:format)                                                                v1/content#update {:format=>/json/}
                          PUT    /v1/content/:id(.:format)                                                                v1/content#update {:format=>/json/}
                          DELETE /v1/content/:id(.:format)                                                                v1/content#destroy {:format=>/json/}
       rails_service_blob GET    /rails/active_storage/blobs/:signed_id/*filename(.:format)                               active_storage/blobs#show
rails_blob_representation GET    /rails/active_storage/representations/:signed_blob_id/:variation_key/*filename(.:format) active_storage/representations#show
       rails_disk_service GET    /rails/active_storage/disk/:encoded_key/*filename(.:format)                              active_storage/disk#show
update_rails_disk_service PUT    /rails/active_storage/disk/:encoded_token(.:format)                                      active_storage/disk#update
     rails_direct_uploads POST   /rails/active_storage/direct_uploads(.:format)                                           active_storage/direct_uploads#create
